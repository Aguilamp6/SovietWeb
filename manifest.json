{
	"name": "Soviet Web",
	"description": "Fix words like \"mine\" or \"yours\" and obliterate the existence of property!",
	"version": "1.3",
	"manifest_version": 2,
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "icon.png"
		}
	},
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
			"js": [
				"jquery-3.5.1.min.js",
				"content.js"
			],
			"matches": [
				"<all_urls>"
			],
			"exclude_matches": [
				"*://*.youtube.com/*",
				"*://tabs.ultimate-guitar.com/tab/*"
			]
		}
	]
}