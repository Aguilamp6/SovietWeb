{
	"name": "Stalin Web",
	"description": "Fix words like 'mine' or 'yours' and obliterate the existence of property!",
	"version": "1.1",
	"manifest_version": 2,
	"icons": {
		"512": "icon.png"
	},
	"content_scripts": [
		{
			"css": [
				"style.css"
			],
			"js": [
				"jquery-3.5.1.min.js",
				"content.js"
			],
			"matches": [
				"<all_urls>"
			],
			"exclude_matches": [
				"*://*.youtube.com/*",
				"*://tabs.ultimate-guitar.com/tab/*"
			]
		}
	]
}