{
	"name": "Chrommunism",
	"description": "Hippity hoppity all words are now our property",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"jquery-3.5.1.min.js",
				"content.js"
			]
		}
	]
}